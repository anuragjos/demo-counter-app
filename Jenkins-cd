pipeline {
     agent any
     parameters{
          choice(name: 'action', choices: 'create\destory\destoryeksclsuter', description: 'Create/Update or destory the eks cluster' )
     }
     stages{
          stage("Git Checkout"){
               steps{
                    git branch: 'devops', url: 'https://github.com/anuragjos/demo-counter-app.git'
               }
          }
     }
}




     
       